{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext", "webworker"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "noEmit": true,
    "esModuleInterop": true,
    "types": ["@cloudflare/workers-types", "@types/jest"]
  },
  "include": ["src/**/*", "globals.d.ts"]
}
